{
  "name": "@myena/vue-table",
  "version": "0.15.2",
  "description": "Vue Table components (client/server) used in ENA projects",
  "author": "Andy Ghiuta <andy.ghiuta@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve --host localhost",
    "build": "node build.js",
    "test:unit": "vue-cli-service test:unit",
    "test:unitw": "vue-cli-service test:unit --watch",
    "test:unit-coverage": "cross-env NODE_ENV=test nyc vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:example": "vue-cli-service build --dest example",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "postversion": "git push && git push --tags && echo Run: \"npm publish\"",
    "preversion": "npm run test:unit && npm run build && git add -A dist"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "src/components/*"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-unit-mocha": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/compiler-sfc": "^3.0.0-beta.14",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^2.0.0-alpha.6",
    "axios": "^0.19.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "bootstrap": "^3.4.1",
    "chai": "^4.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "firebase-tools": "^6.10.0",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jquery": "^3.4.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "nyc": "^15.0.1",
    "qs": "^6.6.0",
    "ramda": "^0.26.1",
    "vue": "^3.0.0-beta.14",
    "vue-cli-plugin-vue-next": "~0.1.2",
    "vuepress": "^1.3.1"
  },
  "peerDependencies": {
    "axios": "^0.18.0",
    "vue": "^3.0.0-beta.14"
  },
  "keywords": [
    "vue",
    "table"
  ],
  "nyc": {
    "include": [
      "src/**/*.{js,ts,vue}"
    ],
    "exclude": [
      "src/*.js",
      "**/entry.js"
    ],
    "reporter": [
      "lcov",
      "clover",
      "text",
      "text-summary"
    ],
    "extension": [
      ".vue",
      ".js",
      ".ts"
    ],
    "cache": true,
    "all": true
  },
  "libname": "EnaTable",
  "license": "MIT",
  "repository": "github:myENA/vue-table"
}
